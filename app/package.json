{
  "name": "node-app-docker-demo-typescript",
  "version": "1.0.0",
  "private": true,
  "description": "Sample node app for educational purposes",
  "main": "dist/index.js",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.4",
    "@types/sinon": "^7.0.13",
    "@types/supertest": "^2.0.8",
    "mocha": "^5.0.0",
    "nodemon": "^1.19.2",
    "nyc": "^14.1.1",
    "sinon": "^4.2.2",
    "supertest": "^3.0.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "typescript": "^3.6.2"
  },
  "scripts": {
    "start": "node .",
    "start-dev": "nodemon --exec ts-node -- ./src/index.ts",
    "test": "mocha --require ts-node/register --exit 'test/**/*.ts'",
    "test-watch": "mocha -w",
    "coverage": "nyc npm test",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build-dev": "tsc",
    "build": "tsc --sourceMap false",
    "prestart": "npm run build"
  },
  "author": "James Barwell <jb@jamesbarwell.co.uk>",
  "license": "MIT"
}
